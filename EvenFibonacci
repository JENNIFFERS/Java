import java.util.*;
import java.util.Scanner;
import java.io.*;
public class EvenFibonacci {
	public static void main(String[] args) {
        int x = 1;
		int y = 2, z;
		long sum = 0;
		while(y < 4000000) {
		    sum += y;
		    z = x+(y<<0x01);
		    y = x+y+z;
		    x = z;
		}
		System.out.println("Sum of the even valued term is "+sum);
	}	
}
