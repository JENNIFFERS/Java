import java.io.*;
import java.util.*;
import java.util.Scanner;
class Ebox_Topic1.2_Design3{
    public static void main(String[] args){
        
        int row;
        Scanner v=new Scanner(System.in);
        row=v.nextInt();
        
        for(int i=row;i>=1; --i){
            for(int j=1;j<=i;++j)
            {
                System.out.print(j+" ");
            }
            System.out.println();
        }
    }
}
